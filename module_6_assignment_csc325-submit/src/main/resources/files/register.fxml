<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="registerPage" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@register.css" fx:controller="com.example.csc325_firebase_webview_auth.view.AccessFBView">
   <children>
      <Text layoutX="270.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Register">
         <font>
            <Font name="System Bold" size="23.0" />
         </font></Text>
      <TextField fx:id="emailInput" layoutX="240.0" layoutY="74.0" promptText="email" />
      <Label layoutX="240.0" layoutY="56.0" text="Email" />
      <Button fx:id="registerButton" layoutX="285.0" layoutY="318.0" mnemonicParsing="false" onAction="#regRecord" text="Register" />
      <TextField fx:id="passwordInput" layoutX="240.0" layoutY="125.0" promptText="password" />
      <Label layoutX="240.0" layoutY="107.0" text="Password" />
      <TextField fx:id="phoneNumberInput" layoutX="240.0" layoutY="176.0" promptText="phone number" />
      <Label layoutX="240.0" layoutY="158.0" text="Phone Number" />
      <TextField fx:id="displayNameInput" layoutX="240.0" layoutY="230.0" promptText="display name" />
      <Label layoutX="240.0" layoutY="212.0" text="Display Name" />
      <TextField fx:id="imageUrlInput" layoutX="240.0" layoutY="281.0" promptText="Image URL" />
      <Label layoutX="240.0" layoutY="263.0" text="Image URL" />
   </children>
</AnchorPane>
